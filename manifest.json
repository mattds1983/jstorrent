{
    "name": "JSTorrent",
    "description": "Javascript Torrent Client",
    "version": "0.3.6",
    "manifest_version": 2,
    "minimum_chrome_version": "24",
    "offline_enabled": true,
    "file_handlers": {
        "any": {
            "types": [
                "application/x-bittorrent"
            ],
            "extensions": [
                "torrent"
            ],
            "title": "Open with JSTorrent"
        }
    },
    "permissions": [
        {"fileSystem":["write"]},
        {"socket":[
            "tcp-connect",
            "udp-bind::",
            "tcp-listen::",
            "udp-send-to::"
        ]},
        "storage",
        "http://*/",
        "https://*/",
        "https://www.google-analytics.com/",
        "https://www.googleapis.com/"
    ],
    "oauth2": {
        "client_id": "432934632994-20rclui1m8od0p2g09vfbrdnk93gbraa.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/drive.file",
                   "https://www.googleapis.com/auth/drive.install"
                  ]
    },
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "icons": { 
        "16": "icon16.png", 
        "48": "icon48.png", 
        "128": "icon128.png" 
    },
    "sandbox": {
        "pages": ["embedded_ga_host.html"]
    }
}
